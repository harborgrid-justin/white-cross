{
  "watch": [
    "src",
    "prisma/schema.prisma"
  ],
  "ext": "ts,js,json",
  "ignore": [
    "dist",
    "node_modules",
    "logs",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "exec": "ts-node --transpile-only src/index.ts",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": "1000",
  "verbose": true,
  "restartable": "rs"
}
