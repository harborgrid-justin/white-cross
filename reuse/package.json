{
  "name": "@white-cross/reusable-function-library",
  "version": "1.0.0",
  "description": "Enterprise-grade reusable function library for White Cross healthcare platform",
  "main": "index.js",
  "scripts": {
    "qa:all": "npm run qa:validate && npm run qa:quality && npm run qa:health && npm run qa:test",
    "qa:validate": "ts-node scripts/validators/run-all-validators.ts",
    "qa:quality": "ts-node scripts/quality/quality-checks.ts",
    "qa:health": "ts-node scripts/health/health-check.ts",
    "qa:test": "jest --coverage",
    "qa:report": "ts-node scripts/reporting/generate-report.ts",
    "qa:metrics": "ts-node scripts/maintenance/kit-statistics.ts",
    "qa:duplicates": "ts-node scripts/maintenance/duplicate-detector.ts",
    "qa:fix": "npm run lint:fix && npm run format",
    "lint": "eslint '**/*.ts' --max-warnings 0",
    "lint:fix": "eslint '**/*.ts' --fix",
    "format": "prettier --write '**/*.ts'",
    "format:check": "prettier --check '**/*.ts'",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "clean": "rimraf dist coverage .qa-reports",
    "find": "grep -r --include='*.ts' --exclude-dir=node_modules --exclude-dir=dist",
    "find:function": "grep -r --include='*.ts' --exclude-dir=node_modules --exclude-dir=dist 'export.*function'",
    "find:class": "grep -r --include='*.ts' --exclude-dir=node_modules --exclude-dir=dist 'export.*class'",
    "list:core": "ls -la core/",
    "list:infrastructure": "ls -la infrastructure/",
    "list:domain": "ls -la domain/",
    "list:all": "find . -name '*.ts' -not -path './node_modules/*' -not -path './dist/*' | sort",
    "search": "echo 'Usage: npm run search \"searchTerm\"' && grep -r --include='*.ts' --exclude-dir=node_modules --exclude-dir=dist -i",
    "docs": "echo 'Opening documentation...' && cat README.md",
    "catalog": "cat FUNCTION-CATALOG.md",
    "navigation": "cat NAVIGATION.md",
    "quick-ref": "cat QUICK-REFERENCE.md"
  },
  "keywords": [
    "nestjs",
    "healthcare",
    "hipaa",
    "enterprise",
    "reusable-functions",
    "white-cross"
  ],
  "author": "White Cross Healthcare",
  "license": "PROPRIETARY",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^61.1.12",
    "jest": "^30.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^6.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/sequelize": "^11.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@types/uuid": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
