{
  "name": "white-cross-nestjs-backend",
  "version": "1.0.0",
  "description": "White Cross School Health Management System - NestJS Backend",
  "author": "White Cross Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "sequelize-cli migration:generate --name",
    "migration:run": "sequelize-cli db:migrate",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "seed:districts": "npm run cli seed:districts",
    "seed:schools": "npm run cli seed:schools",
    "seed:students": "npm run cli seed:students",
    "seed:health-records": "npm run cli seed:health-records",
    "seed:all": "npm run seed:districts && npm run seed:schools && npm run seed:students && npm run seed:health-records",
    "swagger:generate": "npm run start:dev & timeout /t 10 > nul && curl http://localhost:3001/api/docs-json > swagger-spec.json && taskkill /im node.exe /f",
    "swagger:validate": "redocly lint swagger-spec.json",
    "swagger:build": "redocly build-docs swagger-spec.json --output=swagger-docs.html",
    "swagger:preview": "redocly preview-docs swagger-spec.json"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/express5": "^1.1.2",
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.4.3",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/platform-socket.io": "^11.1.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/sequelize": "^11.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/websockets": "^11.1.8",
    "@sentry/tracing": "^7.120.4",
    "@types/email-validator": "^1.0.32",
    "@types/handlebars": "^4.1.0",
    "@types/jspdf": "^2.0.0",
    "@types/nodemailer": "^7.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^11.0.0",
    "apn": "^2.2.0",
    "axios-retry": "^4.5.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "bullmq": "^5.62.0",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "email-validator": "^2.0.4",
    "firebase-admin": "^13.5.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.25.0",
    "handlebars": "^4.7.8",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "libphonenumber-js": "^1.12.25",
    "mysql2": "^3.15.3",
    "nest-commander": "^3.20.1",
    "nodemailer": "^7.0.10",
    "openai": "^6.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.4",
    "web-push": "^3.6.7",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@redocly/cli": "^2.8.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
